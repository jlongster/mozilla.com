RewriteRule ^(\w{2,3}(?:-\w{2}(?:-mac)?)?/)?firefox/3\.5\.\d{1,2}/firstrun[/]?$ $1firefox/3.5/firstrun/ [PT]
RewriteCond $1 !en-US/
RewriteRule ^(\w{2,3}(?:-\w{2}(?:-mac)?)?/)?firefox/3\.5\.\d{1,2}/whatsnew[/]?$ $1firefox/oldversion/ [PT]
